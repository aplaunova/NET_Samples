@model List<CourseModel>
@{
    ViewData["Title"] = "Courses";
}

<h2>Courses list</h2>


<table class="table table-striped">
    <tr>
        <th>Course Name</th>
        <th></th>
    </tr>

    @foreach (var course in Model)
    {
        <tr>
            <td>@course.Course</td>
            <td><a asp-controller="Rating" asp-action="List" asp-route-course="@course.Course">View Details</a></td>
        </tr>
    }


</table>


